<%= form_tag(admin_csv_import_import_path, multipart: true, remote: false) do %>
    <fieldset>
      <legend align="center"><%= t('csv_import.import_products') %></legend>
      <div class="field">
        <%= file_field_tag 'file', accept: 'text/csv' %>
      </div>
      <%= submit_tag t('csv_import.import_button') %>
    </fieldset>
<% end %>
